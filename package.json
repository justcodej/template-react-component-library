{
  "private": false,
  "name": "@justcodej/template-react-component-library",
  "description": "a simple filemanage",
  "version": "0.0.30",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepublishOnly": "npm run build"
  },
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "author": "hao",
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/justcodej"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/justcodej/template-react-component-library.git"
  },
  "files": ["es"],
  "dependencies": {
    "@types/styled-components": "^5.1.25",
    "classnames": "^2.3.1",
    "react": "^16.12.0 || ^17.0.0",
    "react-dom": "^16.12.0 || ^17.0.0",
    "react-transition-group": "^4.4.2",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@types/react": "^18.0.9",
    "@types/react-transition-group": "^4.4.4",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "dumi": "^1.1.0",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "yorkie": "^2.0.0"
  }
}
